{
  "Name": "tactileLocalizer2",
  "Description": "contrasts for the motion localizer dataset",
  "Input": {
    "task": "tactileLocalizer2"
  },
  "Steps": [
    {
      "Level": "run",
      "Model": {
        "X": [
          "trial_type.static",
          "trial_type.motion",
          "trial_type.response",
          "trans_x", "trans_y", "trans_z", "rot_x", "rot_y", "rot_z"
        ]
      },
      "AutoContrasts": [
        "trial_type.static",
        "trial_type.motion" ,
        "trial_type.response"]
    },
    {
      "Level": "subject",
      "AutoContrasts": [
        "trial_type.static",
        "trial_type.motion" ,
        "trial_type.response"],
      "Contrasts": [
        {
          "Name": "motion_gt_static",
          "ConditionList": [
            "trial_type.motion", "trial_type.static"
          ],
          "weights": [1, -1],
          "type": "t"
        },
        {
          "Name": "static_gt_motion",
          "ConditionList": [
            "trial_type.motion", "trial_type.static"
          ],
          "weights": [-1, 1],
          "type": "t"
        }
      ]
    },
    {
      "Level": "dataset",
      "AutoContrasts": ["trial_type.motion", "trial_type.static", "motion_gt_static", "static_gt_motion"]
    }
  ]
}
